		"history": "^5.1.0",
		"joi": "^17.4.2",
		"node-fetch": "^3.1.0",
		"react": "^17.0.2",
		"react-bootstrap": "^2.0.0-rc.1",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.5",
